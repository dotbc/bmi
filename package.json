{
  "name": "bmi",
  "displayName": "BMI",
  "version": "0.0.0",
  "description": "example BMI plugin for dotBC",
  "publisher": "dotBC",
  "main": "index.js",
  "capabilities": {
    "search": {
      "name": "BMI",
      "description": "Look up artists on BMI.",
      "imageUrl": "public/images/icon-bmi.png",
      "searchImageUrl": "public/images/icon-bmi-search.png",
      "supportedSearchTypes": [
        "artist",
        "work"
      ],
      "commands": [
        "search",
        "getDetails"
      ]
    }
  },
  "author": "hi@iammattwalters",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "jsdom": "^9.4.2",
    "lodash": "^4.15.0",
    "string-capitalize-name": "^1.0.3",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "should": "^11.1.0",
    "sinon": "^1.17.5"
  }
}
